<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="androidx.databinding.ObservableMap" />

        <import type="androidx.databinding.ObservableList" />

        <variable
            name="fieldUser"
            type="com.houlin.databinding.observable_data.FieldUser" />

        <variable
            name="map"
            type="ObservableMap&lt;String, Object>" />

        <variable
            name="list"
            type="ObservableList&lt;String>" />

        <variable
            name="user"
            type="com.houlin.databinding.observable_data.ObservableUser" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        tools:context=".observable_data.ObservableActivity">


    </LinearLayout>
</layout>